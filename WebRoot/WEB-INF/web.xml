<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WorkerCtrl</servlet-name>
    <servlet-class>com.lee.ctrl.WorkerCtrl</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TableCtrl</servlet-name>
    <servlet-class>com.lee.ctrl.TableCtrl</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>OrderCtrl</servlet-name>
    <servlet-class>com.lee.ctrl.OrderCtrl</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CustomerCtrl</servlet-name>
    <servlet-class>com.lee.ctrl.CustomerCtrl</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>WorkerCtrl</servlet-name>
    <url-pattern>/WorkerCtrl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TableCtrl</servlet-name>
    <url-pattern>/TableCtrl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderCtrl</servlet-name>
    <url-pattern>/OrderCtrl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CustomerCtrl</servlet-name>
    <url-pattern>/CustomerCtrl</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
